                <action android:name="com.example.android.testview.mothership" />
                <action android:name="com.example.android.testview.radioactivity" />
                <action android:name="com.example.android.testview.rage" />
                <action android:name="com.example.android.testview.peppers" />
                <action android:name="com.example.android.testview.unknown" />
                <action android:name="com.example.android.testview.velvet" />
                <action android:name="com.example.android.testview.war" />


                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
